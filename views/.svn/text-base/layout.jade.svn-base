    link( href="/css/nova_footer.css", media="all", rel="stylesheet" )/
    link( href="/css/style.css", media="all", rel="stylesheet" )/
        
  script(id = "threadTmpl", type = "text/x-jquery-tmpl")
    .thread
      h2 \${title}
      p.desc \${desc}
      p.action What do you think?
      #disqus_thread
    
  body(id=bodyId)
    #container
      header#header
        .inner
          #site-logo
            a(href="/") Open Nova
          nav#top-nav
            ul
              li
                a(href="/ideas") Ideas
              li
                a(href="/collaborate") Collaborate
              li
                a(href="/discuss") Discuss
              li
                a(href="/ask") Ask
              li
                a(href="/wiki") Wiki
              li
                a(href="/article") Article
      #main
        header#main-top
          .inner
            h1 #{pageName} @ Nova
        - if (bodyId == 'idea')
          nav#tags
            .level-1
              .inner
                a#submit-idea(href = "#") Submit Idea
        #content!= body
        
    footer#aim_footer
      .inner
        #copyright
          p Nova is a joint initiative powered by <a href="http://motionworks.com.my">Motionworks</a> &amp; <a href="http://agensi.inovasimalaysia.com">AIM</a>.
          p In conjunction with <a href="http://tandemic.com/">Tandemic</a>.
        #logo_aim
          a(href = "http://agensi.inovasimalaysia.com") AIM: Agensi Innovasi Malaysia
    #sticky_footer
      .inner
        #footer_network
          nav
            ul
              li#footer_home
                a(href = "http://inovasimalaysia.com") Home
              li#footer_learn
                a(href = "http://agensi.inovasimalaysia.com") Learn
                .info
                  p Learn about Innovation and AIM, the agency set up to champion innovation in Malaysia
                  a.more(href = "http://agensi.inovasimalaysia.com") Visit AIM &raquo;
              li#footer_share
                a(href = "http://nova.inovasimalaysia.com") Share
                .info
                  p Share and collaborate with other innovators on Nova, the Open Innovation Platform
                  a.more(href = "http://nova.inovasimalaysia.com") Nova Platform &raquo;
              li#footer_participate
                a(href = "http://weekend.inovasimalaysia.com/") Participate
                .info
                  p Participate and meet other like-minded innovators in the Spark Sessions & Nova Weekends
                  a.more(href = "http://weekend.inovasimalaysia.com/") Nova Weekends &raquo; 
        #footer_social
          nav
            ul
              li#footer_facebook
                a(href = "http://www.facebook.com/NovaMalaysia", title = "Join us on Facebook") Facebook
              li#footer_twitter
                a(href = "http://twitter.com/novamalaysia", title = "Join us on Twitter") Twitter
          #footer_facebook_like
            | <iframe src="http://www.facebook.com/plugins/like.php?href=http%3A%2F%2Fwww.facebook.com%2Fpages%2FOpennova%2F142932372443377&amp;layout=button_count&amp;show_faces=true&amp;width=120&amp;action=like&amp;font=verdana&amp;colorscheme=light&amp;height=21" scrolling="no" frameborder="0" style="border:none; overflow:hidden; width:120px; height:21px;" allowTransparency="true"></iframe>
          
    script( src="http://ajax.googleapis.com/ajax/libs/jquery/1.5.1/jquery.min.js" )
    script( src="https://ajax.googleapis.com/ajax/libs/jqueryui/1.8.11/jquery-ui.min.js" )
    script( src="/js/libs/jquery.nyroModal.custom.min.js?v=1" )
    script( src="/js/libs/jquery.tmpl.min.js?v=1" )
    script( src="/js/plugins.js" )
    script( src="/js/nova_footer.js" )
    - if (bodyId == 'idea')
      script( src="/js/libs/underscore-min.js?v=1" )
      script( src="/js/libs/backbone-min.js?v=1" )
      script( src="/js/libs/jquery.isotope.min.js?v=1" )
      script( src="/js/ideas.js" )
    - else
      script( src="/js/script.js" )
    
