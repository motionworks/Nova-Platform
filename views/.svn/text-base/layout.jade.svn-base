  <!--<![endif]-->
  head
    meta( charset="utf-8" )/
    
    title= title
    meta( content = "Agensi Inovasi Malaysia & Motionworks", name = "author" )/
    meta( content = "Inovasi Malaysia is a national initiative to spur innovation within Malaysia by encouraging people to start taking actions on their ideas.", name = "description" )/
    
    meta(property = "og:title", name= "", http-equiv="", content=title)/
    meta(property = "og:type", name= "", http-equiv="", content="website")/
    meta(property = "og:url", name= "", http-equiv="", content="http://nova.inovasimalaysia.com/")/
    meta(property = "og:image", name= "", http-equiv="", content="http://inovasimalaysia.com/images/nova-logo.png")/
    meta(property = "og:site_name", name= "", http-equiv="", content="Inovasi Malaysia")/
    meta(property = "fb:page_id", name= "", http-equiv="", content="143567002379914")/
    meta(property = "og:description", name= "", http-equiv="", content="Inovasi Malaysia is a national initiative to spur innovation within Malaysia by encouraging people to start taking actions on their ideas.")/
    
