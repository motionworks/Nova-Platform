#idea article.detail .comments { width: 48%; padding: 0; }
#idea article.detail ul { margin: 0; }
#idea article.detail ul img { float: left; margin-right: 1em; }
#idea article.detail ul .name { font-weight: bold; }
#idea article.detail .delete { position: absolute; font-size: 1.2em; right: 3px; top: 3px; font-weight: bold; }

#idea .detail-page { text-align: left; }
#idea .detail-page h1 { font-size: 3em; }
#idea .detail-page h2 { font-size: 2em; }
#idea .detail-page h1, #idea .detail-page h2 { font-family: "HandOfSeanRegular", "Comic Sans MS", "Brush Script MT", cursive, sans-serif; }
#idea .detail-page .idea-author { font-weight: bold; font-size: 1.4em; }
#idea .detail-page p { margin-bottom: 1em; }
#idea .detail-page .desc { font-size: 1.2em; }
#idea .detail-page .comments { padding: 0; }

/*====Main content area====*/
#main { margin-bottom: 0.5em; }

#content { position: relative; }
#content #msg { font-size: 1.2em; width: 80%; border-width: 6px 6px 10px; -moz-border-image: url('../images/border.png?1303789427') 5 5 8 5 stretch; -webkit-border-image: url('../images/border.png?1303789427') 5 5 8 5 stretch; border-image: url('../images/border.png?1303789427') 5 5 8 5 stretch; display: block; margin: 0 auto; background: #ffc; position: relative; cursor: pointer; }
#content #msg_close { font-size: 14px; position: absolute; right: 1em; top: 5px; }

#main-top { background: url('../images/line_hor.png?1303789428') repeat-x bottom; overflow: hidden; }
#main-top #section-counter { text-align: right; padding-top: 5px; }
#main-top #section-counter .number { border: 1px solid #888; padding: 0 5px; }
#main-top h1 { float: left; width: 40%; font-size: 1.8em; padding-bottom: 5px; }
#main-top #topsearch { float: right; text-align: right; padding-top: 11.2px; font-size: 0.8em; }
#main-top #topsearch input[type="submit"] { text-indent: -119988px; overflow: hidden; text-align: left; background-image: url('../images/icon_search.png?1303789428'); background-repeat: no-repeat; background-position: 50% 50%; width: 19px; height: 20px; background-color: #fff; border: none; outline: none; }

/*====Tags====*/
#tags { margin: 0; margin-bottom: 1em; clear: both; }
#tags li { display: -moz-inline-box; -moz-box-orient: vertical; display: inline-block; vertical-align: middle; *vertical-align: auto; }
#tags li { *display: inline; }
#tags li a { display: block; background: #69767b; font-size: 0.8em; padding: 0.2em 0.4em; color: #fff; border: none; }
#tags li.active a, #tags li:hover a { background: #146786; color: #fff; border: none; }
#tags .level-1 { background: url('../images/line_hor.png?1303789428') repeat-x bottom; background-color: #eee; padding-bottom: 5px; overflow: hidden; }
#tags .level-1 .inner > * { display: -moz-inline-box; -moz-box-orient: vertical; display: inline-block; vertical-align: middle; *vertical-align: auto; }
#tags .level-1 .inner > * { *display: inline; }
#tags .level-1 ul { margin-top: 6px; margin-right: 1em; padding-right: 1em; background: url('../images/line_ver.png?1303789428') repeat-y right; }
#tags .level-1 #submit-idea { border-bottom: none; display: -moz-inline-box; -moz-box-orient: vertical; display: inline-block; vertical-align: middle; *vertical-align: auto; background: #0078a4; padding: 0.2em 0.4em; margin-top: 3px; font-size: 1.2em; float: right; color: #fff; }
#tags .level-1 #submit-idea { *display: inline; }
#tags .level-1 #submit-idea:hover { border-bottom: none; background: #0bb1d4; color: #fff; }
#tags .level-2 { background: url('../images/line_hor.png?1303789428') repeat-x bottom; background-color: #ccc; overflow: hidden; }
#tags .level-2 .inner > ul, #tags .level-2 .inner > a { display: -moz-inline-box; -moz-box-orient: vertical; display: inline-block; vertical-align: middle; *vertical-align: auto; }
#tags .level-2 .inner > ul, #tags .level-2 .inner > a { *display: inline; }
#tags .level-2 ul { padding: 0.5em 0 0.8em; }
#tags .level-2 ul li { margin: 1px; vertical-align: top; }
#tags .level-2 #tag-selected { width: 550px; }
#tags .level-2 #tag-selected .active a { background: #269ecc; }
#tags .level-2 #tag-selected .active a:hover { background: #49CDFF; }
#tags .level-2 #tag-selected .delete:hover { color: #f00; }
#tags .level-2 #tag-recommended { width: 240px; padding: 0.5em 2em 0.8em; background: #999; }
#tags .level-2 #tag-refresh { border-bottom: none; display: -moz-inline-box; -moz-box-orient: vertical; display: inline-block; vertical-align: middle; *vertical-align: auto; background: #0078a4; vertical-align: middle; background: #269ecc; font-size: 1.4em; color: #fff; padding: 4px 8px; }
#tags .level-2 #tag-refresh { *display: inline; }
#tags .level-2 #tag-refresh:hover { border-bottom: none; background: #0bb1d4; color: #fff; }
#tags .level-2 #tag-refresh:hover { background: #49cdff; }

@media print { * { background: transparent !important; color: black !important; text-shadow: none !important; filter: none !important; -ms-filter: none !important; }
  a, a:visited { color: #444444 !important; text-decoration: underline; }
  a[href]:after { content: " (" attr(href) ")"; }
  abbr[title]:after { content: " (" attr(title) ")"; }
  .ir a:after, a[href^="javascript:"]:after, a[href^="#"]:after { content: ""; }
  pre, blockquote { border: 1px solid #999; page-break-inside: avoid; }
  thead { display: table-header-group; }
  tr, img { page-break-inside: avoid; }
  @page { margin: 0.5cm; }
  p, h2, h3 { orphans: 3; widows: 3; }
  h2, h3 { page-break-after: avoid; } }
